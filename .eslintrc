{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "comma-dangle": [1, "never"],
    "quotes": [1, "single"],
    "no-unused-vars": [0, {"vars": "local", "args": "none"}],
    "no-alert": 0,
    "no-trailing-spaces": [1, { "skipBlankLines": true }],
    "indent": [2, 2],
    "curly": 2,
    "eqeqeq": 1,
    "no-alert": 1,
    "no-multi-spaces": 1,
    "brace-style": [1, "1tbs"],
    "no-undef": 1,
    "eol-last": 1,
    "no-multiple-empty-lines": 1,
    "no-var": 1
  }
}
